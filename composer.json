{
    "name": "derhansen/powermail_crshield",
    "type": "typo3-cms-extension",
    "description": "Challenge/response spambot protection for TYPO3 ext:powermail",
    "homepage": "https://github.com/derhansen/powermail_crshield",
    "support": {
        "issues": "https://github.com/derhansen/powermail_crshield/issues"
    },
    "authors": [
        {
            "name": "Torben Hansen",
            "email": "derhansen@gmail.com",
            "homepage": "https://www.derhansen.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "TYPO3 CMS",
        "powermail",
        "spam protection",
        "challenge response",
        "extbase"
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^13.4",
        "in2code/powermail": "^13.0"
    },
    "replace": {
        "typo3-ter/powermail-crshield": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Derhansen\\PowermailCrshield\\": "Classes"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "powermail_crshield"
        }
    }
}
